{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",  // Change to "commonjs" instead of "NodeNext"
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowImportingTsExtensions": true,
      "noEmit": true,
    },
    "include": ["src/**/*.ts","types/**/*.ts", "src/config/config.js", "src/config/sequelize-config.js"],
    "exclude": ["node_modules", "dist"]
  }
  